x=0
y=200
def setup ():
    global immage,name
    size(600,400)
    background(255)
    textSize(30)
    rectMode(CENTER)
    noStroke()
    fill(0)
    immage=[loadImage("truba.png"),loadImage("bird.png"),loadImage("gorod.jpg")]
def keyPressed():
    global x,y,immage
    if keyCode == UP:
        y-=10
        image(immage[1],50,y,15,10)
def draw():
    background(255)
    global x,y,immage,name
    if keyCode != UP:
        y+=0.5
    image(immage[x],150,100,300,200)
