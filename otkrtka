def setup():
    size(600,400)
    noStroke()
    fill(0,0,0)
    frameRate(1)
    colorMode(HSB,360,100,100)
    background(0,0,100)
def draw():
    background(0,0,100)
    for a in range(floor(random(5,10))):
        fill(random(0,300),random(0,100),random(0,100))
        circle(random(0,600),random(0,400),random(10,50))
