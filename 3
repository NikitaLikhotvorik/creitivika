def setup():    
    size(600,400)
    background(255,255,255)
    frameRate(24)
    rectMode(CENTER)
    noStroke()
    fill(255,200,0)
def draw():
    background(255,255,255)
    ellipse(300,200,50,50)
    translate(300,200)
    for x in range(8):
        triangle(30,10,30,-10,60,0)
        rotate(radians(360/8))
