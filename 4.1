x=['apple','pine apple','orange','coconut','beef']
y=0
def setup():
    size(600,400)
    background(255,255,255)
    fill(255,0,0)
    textSize(40)
def mouseClicked():
    global y
    if mouseButton == LEFT and y > 0:
        y -= 1
    if mouseButton == RIGHT and y < 4:
        y += 1
def draw():
    global y
    background(255,255,255)
    text(x[y],10,50)
