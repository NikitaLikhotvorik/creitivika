x=[]
pos_y=[]
v=[]
d=[]
def setup():
    size(600,400)
    background(0)
    fill(255,255,255)
    textSize(40)
    noStroke()
    frameRate(4)
def draw():
    global x,pos_y,v,d
    background(50)
    fill(255,255,255)
    for i in range(0,10):
        x.append([floor(random(0,600)),floor(random(5,15))])
        pos_y.append(floor(random(5,15)))
        v[i]=(floor(random(5,15)))




    for i in range(0,len(x)):
        pos_y[i]+=v[i]
        if pos_y[i] > 400:
            del pos_y[i]
            del x[i]
            del v[i]
            del d[i]
    for i in range(0,len(x)):
        ellipse(x[i][0],pos_y[i],x[i][1],x[i][1])
   # fill(100,255,50)
   # triangle(250,350,350,350,300,300)
   # triangle(250,350-40,350,350-40,300,300-40)
   # triangle(250,350-80,350,350-80,300,300-80)
