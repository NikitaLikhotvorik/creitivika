i=600
j=400
l=9
s=9
stolb=20
strok=0
def setup():
    global i,j,l,s
    size(i,j)
    colorMode(HSB, 360,100,100)
    background(100,0,100)
def draw():
    global i,j,l,s,stolb,strok
    stolb=15
    for y in range(stolb,30*s,30):
        for x in range(300-stolb,300+stolb,60):#30,60
            fill(200,x,y)
            ellipse(x,y,30,30)
        stolb+=30
