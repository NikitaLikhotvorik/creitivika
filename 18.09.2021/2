#import keyboard
import pyautogui
import time
list_x = [70,170,270,370,470,600,700,800,950,1050]
x = int(input("число от 1 до 10 "))
pyautogui.hotkey("win", "d")
pyautogui.moveTo(400,800)
pyautogui.click()
pyautogui.moveTo(list_x[x-1],20)
pyautogui.click()
